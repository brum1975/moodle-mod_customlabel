<table class="custombox-authordata">
    <tr valign="top">
        <th class="custombox-title authordata" colspan="2">
            <%%title%%>
        </th>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Auteur(s) :
        </td>
        <td class="custombox-value authordata">
            <%%authors%%><br/>
            <a href="mailto:<%%contact%%>">Contacter les auteurs</a>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Organisation :
        </td>
        <td class="custombox-value authordata">
            <%%organization%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Contributeur(s) :
        </td>
        <td class="custombox-value authordata">
            <%%contributors%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Version :
        </td>
        <td class="custombox-value authordata">
            <%%version%%>
        </td>
    </tr>
</table>