<table class="custombox-authordata">
    <tr valign="top">
        <th class="custombox-title authordata" colspan="2">
            <%%title%%>
        </th>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Author(s):
        </td>
        <td class="custombox-value authordata">
            <%%authors%%><br/>
            <a href="mailto:<%%contact%%>">Contact the author(s)</a>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Support organization:
        </td>
        <td class="custombox-value authordata">
            <%%organization%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Contributor(s):
        </td>
        <td class="custombox-value authordata">
            <%%contributors%%>
        </td>
    </tr>
    <tr valign="top">
        <td class="custombox-param authordata">
            Version:
        </td>
        <td class="custombox-value authordata">
            <%%version%%>
        </td>
    </tr>
</table>